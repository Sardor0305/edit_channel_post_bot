CREATE TABLE "users_chat"(
    "id" SERIAL NOT NULL,
    "chat_id" BIGINT NOT NULL,
    "user_id" BIGINT NOT NULL,
    "text" VARCHAR(255) ,
    "size" INTEGER NOT NULL DEFAULT '64',
    "color" VARCHAR(255) NOT NULL DEFAULT 'white',
    "speed" INTEGER NOT NULL DEFAULT '7',
    "state" VARCHAR(255) NOT NULL DEFAULT 'START'
);
ALTER TABLE
    "users_chat" ADD PRIMARY KEY("id");